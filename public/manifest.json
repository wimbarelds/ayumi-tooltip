{
    "name": "Ayumi-Tooltip",
    "version": "1.0.0",
    "description": "Adds tooltips to champion links",
    "icons": {
        "16": "icon/icon16.png",
        "32": "icon/icon32.png",
        "128": "icon/icon128.png"
    },
    "permissions": [
        "https://ayumilove.net/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://ayumilove.net/*"],
            "js": ["ayumi-tooltip.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "manifest_version": 2
}